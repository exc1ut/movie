{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch index.ts",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["-r", "ts-node/register"],
      "args": ["${workspaceFolder}/server.ts"],
      "env": {
        "PORT": "5000"
      }
    }
  ],
  "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"]
}
